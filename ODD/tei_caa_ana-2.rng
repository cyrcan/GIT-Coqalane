   <define name="tei_cl">
      <element name="cl">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(proposition) représente une proposition grammaticale [17.1. Linguistic Segment Categories]</a:documentation>
