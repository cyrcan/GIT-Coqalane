         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="maxOccurs"
                    a:defaultValue="1">
            <a:documentation>(nombre maximum d'occurrences.) indicates the largest number of times this component may occur.</a:documentation>
            <choice>
               <data type="nonNegativeInteger"/>
               <choice>
