            <!-- AUTRICHE -->
            <witness xml:id="Cod10203"><msDesc>
                  <msIdentifier>
                     <settlement>Vienne</settlement>
                     <institution>Bibliothèque nationale d'Autriche</institution>
                     <repository><geo>48.205909729003906 16.367305755615234</geo></repository>
                     <idno>Codex 10203</idno>
                  </msIdentifier>
                  <physDesc>
                     <!-- Contient 4 coq-à-l'âne -->
                     <p>18 folios, 259x192mm, Petite cursive gothique du XVI<hi rend="sup">e</hi>
                        siècle. Pas de mention de propriétaire. Reliure du XIX<hi rend="sup">e</hi>
                        siècle. (Description par Henri Meylan, 1956, p. XI.)</p>
                     <!-- demande de reproduction intégrale le 4 décembre 2024 obtenue le 18 décembre 2024-->
                  </physDesc>
                  <msContents>
                     <msItem>
                        <p>Epistre du coq a l'asne. "Amy pour bien peu t'esjouir..." f°1-f°5</p>
                     </msItem>
                     <msItem>
                        <p>Aultre epistre du coq a l'asne. "Puis qu'il te plaist parfaict amy..."
                           f°5-f°12</p>
                     </msItem>
                     <msItem>
                        <p>Aultre epistre du coq a l'asne. "Je te veulx escripre a ce coup..."
                           f°12-f°19</p>
                     </msItem>
                     <msItem>
                        <p>Aultre Epistre. "Coq, mon amy, le long sejour..." f°20-f°26</p>
                     </msItem>
                  </msContents>
               </msDesc><listBibl>
                  <bibl>Meylan Henri, 1956.</bibl>
               </listBibl></witness>

